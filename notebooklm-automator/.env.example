# NotebookLM Automator Configuration
# Copy to .env and customize

# Chrome profile directory (persistent session)
# NOTEBOOKLM_CHROME_PROFILE_DIR=~/.notebooklm-chrome

# Chrome remote debugging port
# NOTEBOOKLM_CHROME_DEBUG_PORT=9222

# Run browser in headless mode (not recommended for initial login)
# NOTEBOOKLM_HEADLESS=false

# Default output directory
# NOTEBOOKLM_OUTPUT_DIR=./output

# Timeouts (in seconds)
# NOTEBOOKLM_PAGE_LOAD_TIMEOUT=30
# NOTEBOOKLM_SOURCE_PROCESSING_TIMEOUT=120
# NOTEBOOKLM_AUDIO_GENERATION_TIMEOUT=900
# NOTEBOOKLM_SLIDES_GENERATION_TIMEOUT=180
